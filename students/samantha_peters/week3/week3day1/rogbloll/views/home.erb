<% @post.each do |post| %>
<!-- Title of the post -->
<h2><%= post['title'] %></h2>

<!-- Tagline of the post -->
<h3><%= post['tagline'] %></h3>

<!-- Tagline of the post -->
<p><%= post['postbody'] %></p>

<!-- Author of the post -->
<h4>Created by <%= post['author'] %> </h4>

<!-- Time created -->
<h5>Created on <%= post['created'] %> </h5>
<% end %>
