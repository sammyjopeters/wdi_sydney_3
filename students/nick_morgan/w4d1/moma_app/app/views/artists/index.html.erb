<h1>Artist list</h1>

<a href="/artists/new">New Artist</a>
<!-- <%= link_to 'New Artist', new_artist_path %> -->

<% @artists.each do |artist| %>
  <div>
    <p><%= link_to artist.name, artist %></p>
    <p><%= artist.period %></p>
    <p><%= artist.nationality %></p>
    <p><%= artist.dob %></p>
    <p><%= image_tag artist.image, width: "450" %></p>
  </div>

  <% end %>